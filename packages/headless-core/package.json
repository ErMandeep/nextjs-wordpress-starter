{
  "name": "@webdevstudios/headless-core",
  "version": "1.0.0-canary.2",
  "description": "Next.js WordPress Framework",
  "homepage": "https://webdevstudios.github.io/nextjs-wordpress-starter/",
  "repository": {
    "type": "git",
    "url": "https://github.com/WebDevStudios/nextjs-wordpress-starter/tree/canary/packages/headless-core"
  },
  "author": "WebDevStudios <contact@webdevstudios.com>",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "WP",
    "Headless",
    "CMS",
    "API",
    "WebDevStudios"
  ],
  "type": "module",
  "main": "src/index.mjs",
  "scripts": {
    "build": "npm run clean && npx esbuild 'src/index.mjs' --bundle --loader:.js=jsx --outfile='dist/out.js'",
    "clean": "npx rimraf dist/",
    "format": "npx prettier '**/*.{js,jsx,ts,tsx,md,html,css,scss,json,yml}' --write || true",
    "lint": "npm run format && npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:css": "npx stylelint '**/*.css' --fix || true",
    "lint:js": "npx eslint '**/*.{js,jsx,ts,tsx}' --fix || true",
    "lint:md": "npx markdownlint '**/*.md' --fix || true",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "next": "11.1.2",
    "next-auth": "3.29.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.4.10",
    "apollo-link-rest": "^0.8.0-beta.0",
    "deepmerge": "^4.2.2",
    "graphql": "^15.5.1",
    "graphql-anywhere": "^4.2.7",
    "prop-types": "^15.7.2",
    "react-syntax-highlighter": "^15.4.4",
    "react-twitter-embed": "^3.0.3"
  }
}
